# Docker Development Environment Configuration
COMPOSE_PROJECT_NAME=healthapp-dev

# Environment
NODE_ENV=development
VERSION=dev

# Database Configuration
DB_NAME=healthapp_dev
DB_USER=healthapp_user
DB_PASSWORD=healthapp_pass
DB_PORT=5433

# Redis Configuration
REDIS_PORT=6379

# Application Configuration
JWT_SECRET=dev_jwt_secret_key_change_in_production
FRONTEND_URL=http://localhost:3002
LOG_LEVEL=debug

# API URLs
NEXT_PUBLIC_API_URL=http://localhost:3001/api
BACKEND_PORT=3001
FRONTEND_PORT=3002

# pgAdmin
PGADMIN_EMAIL=admin@healthapp.com
PGADMIN_PASSWORD=admin123
PGADMIN_PORT=5050

# Docker Configuration
VOLUME_MODE=rw
FRONTEND_DOCKERFILE_SUFFIX=.dev
BACKEND_COMMAND=npm run backend:dev

# Payment Processing (Optional - for Stripe integration)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=