{
  "name": "healthapp-template",
  "version": "0.8.4",
  "description": "HealthApp Healthcare Management Platform using Next.js, Node.js, Prisma, and Postgres",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gagneet/healthapp-nextjs.git"
  },
  "keywords": [
    "nextjs",
    "prisma",
    "web",
    "react",
    "mobile",
    "nodejs",
    "postgresql"
  ],
  "author": "gagneet@hotmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gagneet/healthapp-nextjs/issues"
  },
  "homepage": "https://github.com/gagneet/healthapp-nextjs#readme",
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "build:docker": "next build && echo 'Docker build completed - skipping static export for dynamic auth pages'",
    "start": "next start -p 3002",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "find lib -name '*.js' -not -name 'seed.js' -not -name 'seed.cjs' -delete && find app -name '*.js' -delete && find scripts -name '*.js' -delete",
    "clean:all": "find . -name '*.js' -not -path './node_modules/*' -not -name 'seed.js' -not -name 'seed.cjs' -not -path './.next/*' -delete",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:reset": "npx prisma migrate reset --force",
    "seed": "npx prisma db seed",
    "db:generate": "npx prisma generate",
    "db:pull": "npx prisma db pull",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:setup": "npm run db:generate && npm run migrate && npm run seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.17.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.32",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prisma": "^6.14.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.873.0",
    "@daily-co/daily-react": "^0.23.2",
    "@daypilot/daypilot-lite-react": "^4.3.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.1.0",
    "@hookform/resolvers": "^5.0.0",
    "@prisma/client": "^6.14.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/qrcode": "^1.5.5",
    "@types/three": "^0.179.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^4.0.0",
    "dotenv": "^17.0.0",
    "framer-motion": "^11.2.0",
    "glob": "^11.0.3",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "moment": "^2.30.1",
    "next": "^14.2.32",
    "next-auth": "^5.0.0-beta.29",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "postcss": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.0",
    "react-hot-toast": "^2.4.0",
    "recharts": "^2.12.0",
    "redis": "^5.6.1",
    "socket.io": "^4.7.5",
    "stripe": "^14.14.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.0",
    "three": "^0.179.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": ">=22.18.0"
  }
}
