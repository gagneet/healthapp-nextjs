{
  "name": "healthapp-template",
  "version": "4.0.0",
  "description": "HealthApp Healthcare Management Platform using Next.js, Node.js, Prisma, and PostgreSQL",
  "type": "module",
  "main": "src/server.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gagneet/healthapp-nextjs.git"
  },
  "keywords": [
    "nextjs",
    "prisma",
    "web",
    "react",
    "mobile",
    "nodejs",
    "postgresql"
  ],
  "author": "gagneet@hotmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gagneet/healthapp-nextjs/issues"
  },
  "homepage": "https://github.com/gagneet/healthapp-nextjs#readme",
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "build:docker": "next build && echo 'Docker build completed - skipping static export for dynamic auth pages'",
    "start": "next start -p 3002",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "backend:build": "tsc -p tsconfig.backend.json",
    "backend:dev": "nodemon --exec \"node --loader ts-node/esm\" src/server.ts",
    "backend:start": "node --loader ts-node/esm src/server.ts",
    "backend:prod": "npm run backend:build && node dist/server.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:reset": "npx prisma migrate reset --force",
    "seed": "node lib/seed.cjs",
    "seed:all": "node lib/seed.cjs",
    "seed:compile": "tsc lib/seed.ts --outDir lib/ --target es2020 --module commonjs --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck && mv lib/seed.js lib/seed.cjs",
    "seed:dev": "tsx watch lib/seed.ts",
    "seed:prod": "npx prisma db seed",
    "db:generate": "npx prisma generate",
    "db:pull": "npx prisma db pull",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "lint:backend": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:test": "node --import tsx/esm scripts/test-database-setup.ts",
    "db:compare": "node --import tsx/esm scripts/compare-with-target-schema.ts",
    "db:setup": "npm run db:generate && npm run migrate && npm run seed",
    "test:node": "node --import tsx/esm scripts/test-node-compatibility.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.17.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^6.14.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "@daily-co/daily-react": "^0.23.2",
    "@daypilot/daypilot-lite-react": "^4.3.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.1.0",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.14.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/qrcode": "^1.5.5",
    "@types/three": "^0.179.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.0.0",
    "dotenv": "^17.0.0",
    "express": "^4.21.1",
    "express-rate-limit": "^8.0.0",
    "express-validator": "^7.0.1",
    "framer-motion": "^11.2.0",
    "glob": "^11.0.3",
    "helmet": "^8.0.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "next": "^14.2.0",
    "next-auth": "^5.0.0-beta.29",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "otplib": "^12.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "postcss": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.0",
    "react-hot-toast": "^2.4.0",
    "recharts": "^2.12.0",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5",
    "stripe": "^14.14.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.0",
    "three": "^0.179.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": ">=22.18.0"
  }
}
